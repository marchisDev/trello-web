{
    "routes": [
      { "handle": "filesystem" },
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/assets/(.*)", "dest": "/assets/$1" },
      { "src": "/(.*).js", "headers": { "Content-Type": "application/javascript" }, "dest": "/$1.js" },
      { "src": "/.*", "dest": "/index.html" }
    ]
  }